    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch MainJFrame",
            "request": "launch",
            "mainClass": "com.ut.gui.MainJFrame",
            "projectName": "lab-client"
        },
        {
            "type": "java",
            "name": "Launch Server",
            "request": "launch",
            "mainClass": "com.ut.server.server.Server",
            "projectName": "lab-server"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch Client",
            "request": "launch",
            "mainClass": "com.ut.client.Client",
            "projectName": "lab-client"
        }
    ],
    "compounds": [
        {
            "name": "Compound",
            "configurations": ["Launch Client", "Launch Server"]
        }
    ]
}